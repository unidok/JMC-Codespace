	"name": "JustMC Codespace",
	"description": "Утилиты для кодинга на JustMC Creative+.",
	"icon": "assets/icon.png",
	"license": "MIT",
	"authors": [
		"unidok"
	],
	"contact": {
		"sources": "https://github.com/unidok/JMC-Codespace",
